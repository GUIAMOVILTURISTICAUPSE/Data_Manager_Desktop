<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<VBox prefHeight="550.0" prefWidth="840.0" spacing="10.0" stylesheets="/Estilo.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ControllerGoogleMap">
   <children>
      <WebView fx:id="wView" prefHeight="510.0" prefWidth="810.0" />
      <HBox prefHeight="100.0" prefWidth="200.0" spacing="10.0">
         <children>
            <Label styleClass="label" text="LATITUD" />
            <TextField fx:id="txtLatitud" styleClass="textField" />
            <Label styleClass="label" text="LONGITUD" />
            <TextField fx:id="txtLongitud" styleClass="textField" />
            <Button fx:id="btnBuscar" mnemonicParsing="false" styleClass="boton" text="BUSCAR COORDENADAS" />
            <Button fx:id="btnObtener" mnemonicParsing="false" styleClass="boton" text="OBTENER COORDENADAS" />
            <Button fx:id="btnCrearMarcador" mnemonicParsing="false" styleClass="boton" text="CREAR MARCADOR" />
            <Button fx:id="btnRetornar" mnemonicParsing="false" styleClass="boton" text="RETORNAR" />
         </children>
      </HBox>
   </children>
</VBox>
