<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8"
			fx:controller="controllers.ControllerUsuario">
   <children>
      <GridPane layoutY="7.0" prefHeight="175.0" prefWidth="338.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Ingreso de Usuario" />
            <Label text="Nombre De Usuario" GridPane.rowIndex="1" />
            <TextField GridPane.columnIndex="1" GridPane.rowIndex="1" fx:id="tvUsername" />
            <Button mnemonicParsing="false" text="Grabar" GridPane.columnIndex="1" GridPane.rowIndex="2" 
            OnAction="#grabarObjeto"/>
            <Button mnemonicParsing="false" text="Limpiar" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
